{
  "manifest_version": 3,
  "name": "Enter Text",
  "version": "1.0",
  "description": "With this toggle you will be able to press enter buttons in cosutmized ways including automatically.",
  "permissions": [
    "activeTab",
    "storage",
    "notifications"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }
}
